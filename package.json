{
  "name": "preact-hint",
  "version": "0.1.3",
  "source": "src/Hint/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "Tooltip Component for Preact",
  "homepage": "https://github.com/RyanChristian4427/preact-hint",
  "repository": "https://github.com/RyanChristian4427/preact-hint",
  "keywords": [
    "preact",
    "hint",
    "tooltip",
    "component"
  ],
  "sideEffects": false,
  "scripts": {
    "serve:dev": "preact watch -p 3000 --template src/template.html",
    "build": "microbundle --entry src/Hint/index.tsx --css-modules false --tsconfig tsconfig.build.json",
    "lint": "eslint '{src,test}/**/*.{js,ts,tsx}' --fix && prettier '*.json' --write"
  },
  "peerDependencies": {
    "preact": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "microbundle": "^0.12.3",
    "preact": "^10.4.7",
    "preact-cli": "^3.0.1",
    "preact-render-to-string": "^5.1.9",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
