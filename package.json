{
  "name": "preact-hint",
  "version": "0.1.3",
  "source": "src/Hint/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "esmodule": "dist/index.modern.js",
  "umd:main": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "description": "Tooltip Component for Preact",
  "homepage": "https://rschristian.github.io/preact-hint",
  "repository": "https://github.com/rschristian/preact-hint",
  "keywords": [
    "preact",
    "hint",
    "tooltip",
    "component"
  ],
  "sideEffects": false,
  "scripts": {
    "serve:dev": "preact watch -p 3000 --template src/template.html",
    "build": "microbundle --entry src/Hint/index.ts --css-modules false --tsconfig tsconfig.build.json",
    "build:demo": "preact build --template src/template.html --no-prerender --no-sw",
    "serve:demo": "sirv build -p 3000 --single",
    "lint": "eslint '{src,test}/**/*.{js,ts,tsx}' --fix && prettier 'src/**/*.json' '*.json' --write"
  },
  "peerDependencies": {
    "preact": "*"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "bulma": "^0.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "gh-pages": "^3.1.0",
    "microbundle": "^0.12.3",
    "preact": "^10.4.7",
    "preact-cli": "3.0.1",
    "preact-render-to-string": "^5.1.9",
    "prettier": "^2.0.5",
    "sirv-cli": "^1.0.6",
    "typescript": "^3.9.3"
  }
}
